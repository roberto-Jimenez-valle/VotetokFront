{
  "$schema": "https://json.schemastore.org/package",
  "name": "base-sveltekit-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node build/index.js",
    "dev:local": "powershell -ExecutionPolicy Bypass -File scripts/start-dev.ps1",
    "build:clusters": "node scripts/generate-clusters.mjs",
    "dev": "vite dev --host",
    "build": "vite build",
    "postbuild": "node scripts/copy-static-files.js",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "setup": "node scripts/setup-complete.mjs",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "powershell -ExecutionPolicy Bypass -File scripts/start-prisma-studio.ps1",
    "db:reset": "npx prisma migrate reset",
    "db:generate": "npx prisma generate",
    "db:verify-votes": "npx tsx scripts/verify-vote-data-source.ts",
    "db:verify-subdivisions": "npx tsx scripts/verify-subdivision-levels.ts",
    "db:debug-geocode": "npx tsx scripts/debug-geocode.ts",
    "db:debug-topojson": "python scripts/debug_topojson.py",
    "db:populate-subdivisions": "python scripts/populate_subdivisions.py",
    "db:add-teletransporte": "npx tsx scripts/add-teletransporte-votes.ts",
    "cleanup": "node scripts/auto-cleanup.mjs",
    "api:docs": "start http://localhost:5173/api-docs",
    "api:spec": "type src\\lib\\openapi\\spec.yaml"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@mapbox/geojson-rewind": "^0.5.2",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.18.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supercluster": "^7.1.3",
    "@types/swagger-ui-dist": "^3.30.6",
    "@types/three": "^0.180.0",
    "better-sqlite3": "^12.4.1",
    "esbuild": "^0.25.9",
    "lucide-svelte": "latest",
    "postcss": "^8.5",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.16.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "terser": "^5.36.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "d3": "^7.9.0",
    "globe.gl": "^2.44.0",
    "jose": "^6.1.0",
    "pmtiles": "^4.3.0",
    "supercluster": "^8.0.1",
    "swagger-ui-dist": "^5.29.5",
    "three": "^0.180.0",
    "topojson-client": "^3.1.0",
    "yaml": "^2.8.1"
  }
}
