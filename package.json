    "@types/react-dom": "^19",
    "@types/supercluster": "^7.1.3",
    "esbuild": "^0.25.9",
    "lucide-svelte": "latest",
    "postcss": "^8.5",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
