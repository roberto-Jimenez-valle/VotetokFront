{
  "$schema": "https://json.schemastore.org/package",
  "name": "base-sveltekit-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build:clusters": "node scripts/generate-clusters.mjs",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
